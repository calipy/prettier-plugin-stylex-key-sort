{
  "name": "@calipy/prettier-plugin-stylex-key-sort",
  "author": "srikrsna",
  "version": "1.0.3",
  "description": "Prettier plugin to sort StyleX keys",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/calipy/prettier-plugin-stylex-key-sort",
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "lint": "eslint \"**/*.{ts,js}\"",
    "lint:fix": "npm run lint --fix",
    "format": "prettier --write '**/*.{ts,json}' --config .prettierrc.json --ignore-path .gitignore",
    "format:check": "prettier --check '**/*.{ts,json}' --config .prettierrc.json --ignore-path .gitignore"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "prettier",
    "plugin",
    "stylex",
    "sort keys"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/types": "^7.24.0",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@stylexjs/shared": "^0.5.1",
    "prettier": "^3.2.5"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "packageManager": "pnpm@9.12.1+sha512.e5a7e52a4183a02d5931057f7a0dbff9d5e9ce3161e33fa68ae392125b79282a8a8a470a51dfc8a0ed86221442eb2fb57019b0990ed24fab519bf0e1bc5ccfc4"
}
